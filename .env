TRADE_AMOUNT_SOL=0.10
MAX_POSITIONS=3
MIN_SAFETY_SCORE=50
MIN_VOLUME_24H=10000
MIN_HOLDERS=0
MIN_LIQUIDITY=8000
TOKEN_AGE_HOURS_MIN=0
TOKEN_AGE_HOURS_MAX=24
SCAN_INTERVAL_SECONDS=10
MAX_TOKENS_PER_SCAN=25
